{
    "name": "wingsergey/config-access-bundle",
    "description": "Get/set your application config values using dot paths.",
    "keywords": ["config", "read", "write", "access"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin CHALAS",
            "email": "robin.chalas@gmail.com",
            "homepage": "https://github.com/chalasr"
        }
    ],
    "require": {
        "php": ">= 5.4.0",
        "doctrine/collections": "~1.3",
        "symfony/framework-bundle": "~2.8|~3.1|~4.0",
        "symfony/dependency-injection": "~2.8|~3.1|~4.0",
        "symfony/config": "~2.8|~3.1|~4.0",
        "symfony/expression-language": "~2.8|~3.1|~4.0",
        "symfony/http-kernel": "~2.8|~3.1|~4.0",
        "symfony/cache": "~3.1|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "symfony/expression-language": "For injecting configuration values as service arguments"
    },
    "autoload": {
        "psr-4": { "RCH\\ConfigAccessBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
